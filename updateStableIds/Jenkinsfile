pipeline {
    agent {
        docker {
            image 'maven:3-alpine'
            args '-v /root/.m2:/root/.m2'
        }
    }
    stages {
        stage('Build') {
            steps {
            dir('updateStableIds') {
    	         sh 'mvn clean compile assembly:single'
               sh 'cat src/main/resources/config.properties'
                }
            }
        }
    }
}
